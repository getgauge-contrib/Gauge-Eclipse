grammar io.getgauge.Spec with org.eclipse.xtext.common.Terminals

generate spec "http://www.getgauge.io/Spec"

Spec:
	'#' name=SENTENCE
	entities+=Entity*
;

Entity:
	Scenario | Step
;

Scenario:
	'##' name=SENTENCE
	steps+=Step*
;

Step:
	'*' name=SENTENCE
;

terminal SENTENCE : ( '\\' . /* 'b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\' */ | !('\\'|'"') )*;