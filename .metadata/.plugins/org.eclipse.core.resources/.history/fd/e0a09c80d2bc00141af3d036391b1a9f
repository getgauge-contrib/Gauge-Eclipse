package com.thoughtworks.gauge.serializer;

import com.google.inject.Inject;
import com.thoughtworks.gauge.services.EclipseGrammarAccess;
import java.util.List;
import org.eclipse.emf.ecore.EObject;
import org.eclipse.xtext.IGrammarAccess;
import org.eclipse.xtext.RuleCall;
import org.eclipse.xtext.nodemodel.INode;
import org.eclipse.xtext.serializer.analysis.GrammarAlias.AbstractElementAlias;
import org.eclipse.xtext.serializer.analysis.GrammarAlias.TokenAlias;
import org.eclipse.xtext.serializer.analysis.ISyntacticSequencerPDAProvider.ISynNavigable;
import org.eclipse.xtext.serializer.analysis.ISyntacticSequencerPDAProvider.ISynTransition;
import org.eclipse.xtext.serializer.sequencer.AbstractSyntacticSequencer;

@SuppressWarnings("all")
public class EclipseSyntacticSequencer extends AbstractSyntacticSequencer {

	protected EclipseGrammarAccess grammarAccess;
	protected AbstractElementAlias match_Scenario_HyphenMinusKeyword_0_1_1_a;
	protected AbstractElementAlias match_Spec_EqualsSignKeyword_0_1_1_a;
	
	@Inject
	protected void init(IGrammarAccess access) {
		grammarAccess = (EclipseGrammarAccess) access;
		match_Scenario_HyphenMinusKeyword_0_1_1_a = new TokenAlias(true, true, grammarAccess.getScenarioAccess().getHyphenMinusKeyword_0_1_1());
		match_Spec_EqualsSignKeyword_0_1_1_a = new TokenAlias(true, true, grammarAccess.getSpecAccess().getEqualsSignKeyword_0_1_1());
	}
	
	@Override
	protected String getUnassignedRuleCallToken(EObject semanticObject, RuleCall ruleCall, INode node) {
		return "";
	}
	
	
	@Override
	protected void emitUnassignedTokens(EObject semanticObject, ISynTransition transition, INode fromNode, INode toNode) {
		if (transition.getAmbiguousSyntaxes().isEmpty()) return;
		List<INode> transitionNodes = collectNodes(fromNode, toNode);
		for (AbstractElementAlias syntax : transition.getAmbiguousSyntaxes()) {
			List<INode> syntaxNodes = getNodesFor(transitionNodes, syntax);
			if(match_Scenario_HyphenMinusKeyword_0_1_1_a.equals(syntax))
				emit_Scenario_HyphenMinusKeyword_0_1_1_a(semanticObject, getLastNavigableState(), syntaxNodes);
			else if(match_Spec_EqualsSignKeyword_0_1_1_a.equals(syntax))
				emit_Spec_EqualsSignKeyword_0_1_1_a(semanticObject, getLastNavigableState(), syntaxNodes);
			else acceptNodes(getLastNavigableState(), syntaxNodes);
		}
	}

	/**
	 * Syntax:
	 *     '-'*
	 */
	protected void emit_Scenario_HyphenMinusKeyword_0_1_1_a(EObject semanticObject, ISynNavigable transition, List<INode> nodes) {
		acceptNodes(transition, nodes);
	}
	
	/**
	 * Syntax:
	 *     '='*
	 */
	protected void emit_Spec_EqualsSignKeyword_0_1_1_a(EObject semanticObject, ISynNavigable transition, List<INode> nodes) {
		acceptNodes(transition, nodes);
	}
	
}
